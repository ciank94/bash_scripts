#Aliases for git
alias gist='git status'
#alias giadd='git add'
alias giad='git add --all'
alias gico='git commit'
alias gilo='git log --oneline --decorate'
alias gisa='git log>dummy.txt && mv -i dummy.txt gitlog.txt'
alias gigr='git log --oneline --decorate --graph --all'
#alias gila='git log -1 HEAD'
alias gich='git checkout'
alias gipu='git push'
alias gibr='git branch'
alias gidi='git diff'
